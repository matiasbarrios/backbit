		"@radix-ui/react-icons": "^1.3.2",
		"@radix-ui/themes": "^3.2.1",
		"@vitejs/plugin-react": "^4.3.4",
		"electron": "^37.2.3",
		"electron-builder": "^25.1.8",
		"electron-vite": "^4.0.0",
		"eslint": "^9.31.0",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-react": "^7.37.5",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-router-dom": "^7.8.1",
		"vite": "^7.0.5"
